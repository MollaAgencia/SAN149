                        </div>
                        <div class="form-group col">
                            <label class="mb-0">Confirmar senha <span class="required text-4">*</span></label>
                            <div class="input-group">
                                <input name="" id="cadas-conf-senha" type="password" class="form-control form-control-lg" valida-confsenha required>
                            </div>
                            <div class="alertSenha" alert-conf-senha></div>
                        </div>
                    </div>

                    <div class="form-check d-none">
                        <input class="form-check-input" type="checkbox" value="" valid="" id="regulamento" valida-checkbox required>
